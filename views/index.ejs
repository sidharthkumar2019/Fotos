<% if (images.length>0){ %>
    <div class="container" style="width: 100%;">
        <div class="row">
            <% images.forEach(function(image){ %>
                <div class="card" style="width: 20rem; margin: 10px;">
                    <img class="card-img-top" src="/<%= image.img %>?tn=1" alt="<%= image.name %>"/>
                    <div class="card-body">                
                        <h5 class="card-title"><%=image.name%></h5>
                        <h6 class="card-subtitle mb-2 text-muted"><%=image.category.name%></h6>
                        <p class="card-text"><%=image.description%></p>
                        <a href="<%=image.img%>" target="_blank" class="btn btn-primary">At glance</a>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
<% } %>
  